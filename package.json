{
  "name": "jwt-auth",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "build": "npm install  && cd frontend && npm install && npm run build",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "docker-build-client": "NPM_CONFIG_PRODUCTION=false cd frontend && npm install && npm run build",
    "docker-build-webapp": "concurrently \"npm run docker-build-client\" \"npm run server\""
  },
  "author": "",
  "license": "ISC",
